{
    "name": "jmolinas/support",
    "description": "Support packages for Lumen projects",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/pagination": "^10.0",
        "illuminate/encryption": "^10.0",
        "tymon/jwt-auth": "^2.0",
        "webpatser/laravel-uuid": "^4.0",
        "illuminate/events": "^10.0",
        "illuminate/mail": "^10.0",
        "rinvex/countries": "^9.0",
        "illuminate/validation": "^10.17"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "psr-4": {
            "JMolinas\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "Devs",
            "email": "devs@email.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
