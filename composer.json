{
    "name": "geniusprime/support",
    "description": "Support packages for Genius Prime Projects",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/auth": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/pagination": "^7.0",
        "illuminate/encryption": "^7.0",
        "nikic/fast-route": "^1.3",
        "symfony/var-dumper": "^4.1",
        "webpatser/laravel-uuid": "^3.0",
        "tymon/jwt-auth": "~1.0",
        "phpgangsta/googleauthenticator": "dev-master",
        "vlucas/phpdotenv": "^4.0",
        "illuminate/events": "^7.0",
        "illuminate/mail": "^7.0",
        "rinvex/countries": "^6.0",
        "barryvdh/laravel-cors": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "GP\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "authors": [
        {
            "name": "GP Devs",
            "email": "devs@email.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
